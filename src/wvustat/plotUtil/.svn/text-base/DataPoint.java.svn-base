package wvustat.plotUtil;

/**
 *
 * @author  hxue
 * @version 
 */
public class DataPoint extends Object {
    private double x, y;

    /** Creates new DataPoint */
    public DataPoint(double x, double y) {
        this.x=x;
        this.y=y;
    }
    
    public double getX(){
        return x;
    }
    
    public double getY(){
        return y;
    }
    
    public void setX(double x){
        this.x=x;
    }
    
    public void setY(double y){
        this.y=y;
    }
}