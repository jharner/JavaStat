package wvustat.statistics;

/**
 *
 * @author  James Harner
 * @version 
 */
public class InvalidDataError extends java.lang.Exception {

    /**
 * Creates new <code>InvalidDataError</code> without detail message.
     */
    public InvalidDataError() {
    }


    /**
 * Constructs an <code>InvalidDataError</code> with the specified detail message.
     * @param msg the detail message.
     */
    public InvalidDataError(String msg) {
        super(msg);
    }
}


