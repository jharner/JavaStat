package wvustat.table;

/**
 *
 * @author  hxue
 * @version 
 */
/**
 * ParseException represents an error condition in the process of parsing.
 *
 * @author  Hengyi Xue
 * @version 1.0, July 31, 2000
 */
public class ParseException extends Exception {

    /**
     * Creates new <code>ParseException</code> without detail message.
     */
    public ParseException() {
    }


    /**
     * Constructs an <code>ParseException</code> with the specified detail message.
     * @param msg the detail message.
     */
    public ParseException(String msg) {
        super(msg);
    }
}
